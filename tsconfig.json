{
  "compilerOptions": {
    "sourceMap": true,
    "noImplicitAny": true,
    "allowUnusedLabels": false,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "module": "commonjs",
    "target": "es5",
    "jsx": "react",
    "lib": ["es6", "es2016", "dom", "esnext"],
    "types": [
      "history",
      "immutable",
      "material-ui",
      "node",
      "react",
      "react-dom",
      "react-redux",
      "react-router-dom",
      "react-router-redux",
      "redux",
      "redux-logger",
      "reselect",
      "meteor"
    ],
    "removeComments": true,
    "experimentalDecorators": true
  }
}
